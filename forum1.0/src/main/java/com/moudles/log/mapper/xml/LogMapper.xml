<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moudles.log.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.moudles.log.entity.Log">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="message" property="message" />
        <result column="type" property="type" />
        <result column="time" property="time" />
        <result column="spare1" property="spare1" />
        <result column="spare2" property="spare2" />
        <result column="spare3" property="spare3" />
        <result column="spare4" property="spare4" />
        <result column="spare5" property="spare5" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, message, type, time, spare1, spare2, spare3, spare4, spare5
    </sql>

</mapper>
